        <mxCell id="FD6mRzqT4uLZpE83utRG-9" value="&lt;span style=&quot;font-size: 14px;&quot;&gt;それが正解なんです&lt;br style=&quot;font-size: 14px;&quot;&gt;&lt;/span&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontSize=14;spacing=10;fontStyle=1;fillColor=#f8cecc;strokeColor=#b85450;" parent="1" vertex="1">
