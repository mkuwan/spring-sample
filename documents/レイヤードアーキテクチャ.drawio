        <mxCell id="rDD6o5QnM6w241JWopNN-81" value="ViewModel&lt;br&gt;Controller" style="rounded=1;whiteSpace=wrap;html=1;fontSize=14;strokeColor=#d6b656;fillColor=#fff2cc;dashed=1;" parent="1" vertex="1">
          <mxGeometry x="152" y="2144" width="210" height="90" as="geometry" />
