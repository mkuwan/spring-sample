        <mxCell id="rDD6o5QnM6w241JWopNN-140" value="public class ClassA&lt;br style=&quot;font-size: 12px;&quot;&gt;{&lt;br&gt;&lt;br style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; private readonly ICustomerService _customerService;&lt;br style=&quot;font-size: 12px;&quot;&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; // コンストラクタインジェクション&lt;br&gt;&amp;nbsp; &amp;nbsp; public ClassA (ICustomerSerivce customerSerivce)&lt;br&gt;&amp;nbsp; &amp;nbsp; {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;_customerService = customerService;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;}&lt;br style=&quot;font-size: 12px;&quot;&gt;&lt;br style=&quot;font-size: 12px;&quot;&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; // インターフェースのメソッドを使用してみる&lt;br&gt;&amp;nbsp; &amp;nbsp; public void Method()&lt;br&gt;&amp;nbsp; &amp;nbsp; {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; _customerSerivce.FindCustomer(.....);&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;}&lt;br style=&quot;font-size: 12px;&quot;&gt;&lt;br style=&quot;font-size: 12px;&quot;&gt;}" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontFamily=Mayryo;fontSize=12;fontColor=#000000;" parent="rDD6o5QnM6w241JWopNN-125" vertex="1">
