        <mxCell id="It1XWHNwRKQV5UImB5jQ-6" value="&lt;span style=&quot;font-size: 14px;&quot;&gt;1つのメソッドは可能な限り短くする&lt;br&gt;&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=12;spacing=10;fontStyle=0;fillColor=default;gradientColor=none;strokeColor=#0000CC;" parent="1" vertex="1">
