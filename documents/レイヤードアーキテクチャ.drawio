        <mxCell id="It1XWHNwRKQV5UImB5jQ-7" value="&lt;span style=&quot;font-size: 14px;&quot;&gt;１メソッド１機能にする&lt;br&gt;&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=12;spacing=10;fontStyle=0;fillColor=default;gradientColor=none;strokeColor=#0000CC;" parent="1" vertex="1">
